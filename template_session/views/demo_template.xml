<odoo>
    <template id="demo_page" name="Demo page">
        <t t-call="website.layout">
            <t t-set="title">Demo Page</t>
            <div class="oe_structure">
                <div class="container">
                    <table id="partners_list">
                        <thead class="thead-dark">
                            <tr>
                                <th>Profile</th>
                                <th>Name</th>
                                <th>Email</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr t-foreach="partners" t-as="pt">
                                <td>
                                    <!-- <t t-esc="pt.image_1920" t-options="{'widget':'image'}"/> -->
                                    <img t-if="pt.image_1920" t-att-src="'data:image/jpeg;base64,%s' % to_text(pt.image_1920)"/>
                                </td>
                                <td>
                                    <t t-esc="pt.name"/>
                                </td>
                                <td>
                                    <t t-esc="pt.email"/>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </t>
    </template>
</odoo>
